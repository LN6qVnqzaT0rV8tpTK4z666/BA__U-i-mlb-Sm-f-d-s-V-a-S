{
  "git.ignoreLimitWarning": true,
  "latex-workshop.latex.outDir": "build",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk (build dir)",
      "tools": ["latexmk"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-interaction=nonstopmode",
        "-synctex=1",
        "-output-directory=build",
        "%DOC%"
      ]
    }
  ]
}